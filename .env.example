# Copy this file to .env and fill in your values
# Do not commit .env to version control
# This file is only for debugging and local development
# It is unneeded in production / Kubernetes deployments

# Environment: "development" or "production"
ENVIRONMENT=development

# Database connection (Format: mysql+asyncmy://user:password@host:port/database)
DEV_DB_URL=mysql+asyncmy://solsbot_user:YOUR_PASSWORD@localhost:3306/solsbot_helper

# Development bot token (used when ENVIRONMENT=development)
BOT_TOKEN_DEV=YOUR_DEVELOPMENT_BOT_TOKEN

# Sol's RNG API token
SOLS_BOT_TOKEN=YOUR_SOLS_API_TOKEN

# Optional debug webhook
OWNER_WEBHOOK_DEBUG_URL=
